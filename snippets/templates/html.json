{
    "_comment" : "This section is autocomplete for django templates",

    "variable": {
        "description": "{{ var }}",
        "prefix": "variable",
        "body": [
            "{{ $1 }}"
        ]
    },

    "tag": {
        "description": "{% tag %}",
        "prefix": "tag",
        "body": [
            "{% $1 %}"
        ]
    },

    "autoescape": {
        "description": "{% autoescape %}",
        "prefix": "autoescape",
        "body": [
            "{% autoescape ${1|off,on|} %}$0",
            "{% endautoescape %}"
        ]
    },

    "endautoescape": {
        "description": "{% endautoescape %}",
        "prefix": "endautoescape",
        "body": "{% endautoescape %}"
    },

    "block": {
        "description": "{% block %}",
        "prefix": "block",
        "body": [
            "{% block $1 %}$0",
            "{% endblock $1 %}"
        ]
    },

    "block_inline": {
        "description": "{% block %} (inline)",
        "prefix": "blok",
        "body": "{% block $1 %}$0{% endblock %}"
    },

    "endblock": {
        "description": "{% endblock %}",
        "prefix": "endblock",
        "body": "{% endblock %}"
    },

    "comment_inline": {
        "description": "{% comment %} (inline)",
        "prefix": "comm",
        "body": "{# $1 #}"
    },

    "comment": {
        "description": "{% comment %}",
        "prefix": "comment",
        "body": [
            "{% comment \"$1\" %}$0",
            "{% endcomment %}"
        ]
    },

    "comment_paste": {
        "description": "{% comment %} (paste)",
        "prefix": "comment",
        "body": [
            "{% comment %}${CLIPBOARD}",
            "{% endcomment %}"
        ]
    },

    "comment_selection": {
        "description": "{% comment %} (selection)",
        "prefix": "comment",
        "body": [
            "$0{% comment %}",
            "\t${TM_SELECTED_TEXT}",
            "$0{% endcomment %}"
        ]
    },

    "endcomment": {
        "description": "{% endcomment %}",
        "prefix": "endcomment",
        "body": "{% endcomment %}"
    },

    "csrf_token": {
        "description": "{% csrf_token %}",
        "prefix": "csrf_token",
        "body": "{% csrf_token %}"
    },

    "cycle": {
        "description": "{% cycle %}",
        "prefix": "cycle",
        "body": [
            "{% cycle $1 %}$0",
            "{% endcycle %}"
        ]
    },

    "endcycle": {
        "description": "{% endcycle %}",
        "prefix": "endcycle",
        "body": "{% endcycle %}"
    },

    "debug": {
        "description": "{% debug %}",
        "prefix": "debug",
        "body": [
            "{% debug $1 %}$0",
            "{% enddebug %}"
        ]
    },

    "enddebug": {
        "description": "{% enddebug %}",
        "prefix": "enddebug",
        "body": "{% enddebug %}"
    },

    "extends": {
        "description": "{% extends %}",
        "prefix": "extends",
        "body": [
            "{% extends \"$1\" %}$0"
        ]
    },

    "filter": {
        "description": "{% filter %}",
        "prefix": "filter",
        "body": [
            "{% filter $1 %}$0",
            "{% endfilter %}"
        ]
    },

    "endfilter": {
        "description": "{% endfilter %}",
        "prefix": "endfilter",
        "body": "{% endfilter %}"
    },

    "firstof": {
        "description": "{% firstof %}",
        "prefix": "firstof",
        "body": [
            "{% firstof $0 %}"
        ]
    },

    "endfirstof": {
        "description": "{% endfirstof %}",
        "prefix": "endfirstof",
        "body": "{% endfirstof %}"
    },

    "for": {
        "description": "{% for %}",
        "prefix": "for",
        "body": [
            "{% for $1 %}$0",
            "{% endfor %}"
        ]
    },

    "endfor": {
        "description": "{% endfor %}",
        "prefix": "endfor",
        "body": "{% endfor %}"
    },

    "forempty": {
        "description": "{% forempty %}",
        "prefix": "forempty",
        "body": [
            "{% for $1 %}$0",
            "{% empty %}$2",
            "{% endfor %}"
        ]
    },

    "if": {
        "description": "{% if %}",
        "prefix": "if",
        "body": [
            "{% if $1 %}$0",
            "{% endif %}"
        ]
    },

    "ifelse": {
        "prefix": "ifelse",
        "body": [
            "{% if $1 %}$2",
            "{% else %}$3",
            "{% endif %}"
        ],
        "description": "if... else..."
    },

    "elif": {
        "prefix": "elif",
        "body": "{% elif %}%0",
        "description": "elif"
    },
    
    "else": {
        "prefix": "else",
        "body": "{% else %}",
        "description": "else"
    },

    "endif": {
        "description": "{% endif %}",
        "prefix": "endif",
        "body": "{% endif %}"
    },

    "ifchanged": {
        "description": "{% ifchanged %}",
        "prefix": "ifchanged",
        "body": [
            "{% ifchanged $1 %}$0",
            "{% endifchanged %}"
        ]
    },

    "endifchanged": {
        "description": "{% endifchanged %}",
        "prefix": "endifchanged",
        "body": "{% endifchanged %}"
    },

    "include": {
        "description": "{% include %}",
        "prefix": "include",
        "body": [
            "{% include $1 %}"
        ]
    },

    "load": {
        "description": "{% load tags %}",
        "prefix": "load",
        "body": [
            "{% load ${1|static,i18n,cache,l10n,tz,humanize,flatpages,admin_list,admin_modify,admin_static,admin_urls,base,log|} %}"
        ]
    },

    "lorem": {
        "description": "{% lorem %}",
        "prefix": "lorem",
        "body": [
            "{% lorem $1 %}$0",
            "{% endlorem %}"
        ]
    },

    "endlorem": {
        "description": "{% endlorem %}",
        "prefix": "endlorem",
        "body": "{% endlorem %}"
    },

    "now": {
        "description": "{% now %}",
        "prefix": "now",
        "body": [
            "{% now $1 %}$0",
            "{% endnow %}"
        ]
    },

    "endnow": {
        "description": "{% endnow %}",
        "prefix": "endnow",
        "body": "{% endnow %}"
    },

    "regroup": {
        "description": "{% regroup %}",
        "prefix": "regroup",
        "body": "{% regroup $1 by $2 as ${3:_list} %}"
    },

    "regroup_example": {
        "description": "{% regroup %}",
        "prefix": "regroup_example",
        "body": [
            "{% regroup ${1:object_list} by ${2:category} as ${2:category}_list %}",
            "<ul>",
            "    {% for ${2:category}, ${3:obj}_list in ${2:category}_list %}",
            "        <li>{{ ${2:category} }}",
            "        <ul>",
            "            {% for ${3:obj} in ${3:obj}_list %}",
            "                <li>{{ ${3:obj} }}</li>",
            "            {% endfor %}",
            "        </ul>",
            "        </li>",
            "    {% endfor %}",
            "</ul>"
        ]
    },

    "resetcycle": {
        "description": "{% resetcycle %}",
        "prefix": "resetcycle",
        "body": "{% resetcycle %}"
    },

    "spaceless": {
        "description": "{% spaceless %}",
        "prefix": "spaceless",
        "body": [
            "{% spaceless $1 %}$0",
            "{% endspaceless %}"
        ]
    },

    "endspaceless": {
        "description": "{% endspaceless %}",
        "prefix": "endspaceless",
        "body": "{% endspaceless %}"
    },

    "templatetag": {
        "description": "{% templatetag %}",
        "prefix": "templatetag",
        "body": [
            "{% templatetag ${1|openblock,closeblock,openvariable,closevariable,openbrace,closebrace,opencomment,closecomment|} %}"
        ]
    },

    "url": {
        "description": "{% url %}",
        "prefix": "url",
        "body": "{% url \"$1\" %}"
    },

    "urlpk": {
        "description": "{% url pk=object.pk %}",
        "prefix": "urlpk",
        "body": "{% url \"$1\" pk=${2:${3:object}.pk} %}"
    },

    "urlslug": {
        "description": "{% url slug=object.slug %}",
        "prefix": "urlslug",
        "body": "{% url \"$1\" slug=${2:${3:object}.slug} %}"
    },

    "verbatim": {
        "description": "{% verbatim %}",
        "prefix": "verbatim",
        "body": [
            "{% verbatim $1 %}$0",
            "{% endverbatim %}"
        ]
    },

    "endverbatim": {
        "description": "{% endverbatim %}",
        "prefix": "endverbatim",
        "body": "{% endverbatim %}"
    },

    "widthratio": {
        "description": "{% widthratio %}",
        "prefix": "widthratio",
        "body": [
            "{% widthratio $1 %}$0",
            "{% endwidthratio %}"
        ]
    },

    "endwidthratio": {
        "description": "{% endwidthratio %}",
        "prefix": "endwidthratio",
        "body": "{% endwidthratio %}"
    },

    "with": {
        "description": "{% with %}",
        "prefix": "with",
        "body": [
            "{% with $1 as $2 %}$0",
            "{% endwith %}"
        ]
    },

    "endwith": {
        "description": "{% endwith %}",
        "prefix": "endwith",
        "body": "{% endwith %}"
    },

    "with_selection": {
        "description": "{% with as %} (selection)",
        "prefix": "with_selection",
        "body": [
            "$0{% with $1 as $2 %}",
            "\t${TM_SELECTED_TEXT}",
            "$0{% endwith %}"
        ]
    },

    "with_paste": {
        "description": "{% with as %} (paste)",
        "prefix": "with_paste",
        "body": [
            "{% with $1 as $2 %}${CLIPBOARD}",
            "{% endwith %}"
        ]
    },

    "trans": {
        "description": "{% trans \"\" %}",
        "prefix": "trans",
        "body": [
            "{% trans \"${0:$SELECTION}\" %}"
        ]
    },

    "trans_paste": {
        "description": "{% trans \"(paste)\" %}",
        "prefix": "trans_paste",
        "body": [
            "{% trans \"${CLIPBOARD}\" %}"
        ]
    },

    "blocktrans": {
        "description": "{% blocktrans %}{% endblocktrans %}",
        "prefix": "blocktrans",
        "body": [
            "{% blocktrans %}$0",
            "{% endblocktrans %}"
        ]
    },

    "blocktrans_paste": {
        "description": "{% blocktrans %}(paste){% endblocktrans %}",
        "prefix": "blocktrans_paste",
        "body": [
            "{% blocktrans ${2:trimmed }%}",
            "    ${CLIPBOARD}$1",
            "{% endblocktrans %}"
        ]
    },

    "blocktrans_with": {
        "description": "{% blocktrans as %}{% endblocktrans %}",
        "prefix": "blocktrans_with",
        "body": [
            "{% blocktrans with $1=$2 ${3:trimmed }%}$0",
            "{% endblocktrans %}"
        ]
    },

    "static": {
        "prefix": "static",
        "body": "{% static \"${1:$SELECTION}\" %}",
        "description": "static"
    },

    "super": {
        "description": "{{ block.super }}",
        "prefix": "super",
        "body": "{{ block.super }}"
    }
}